&GLOBAL
  PRINT_LEVEL LOW
  PROJECT dimer
  RUN_TYPE MD
&END GLOBAL

&MOTION
  &MD
    ENSEMBLE NVE
    STEPS 5
    TEMPERATURE 330.0
    TIMESTEP 0.48
  &END MD
  &PRINT
    &TRAJECTORY
      PRINT_ATOM_KIND T
      &EACH
        MD 1
      &END EACH
    &END TRAJECTORY
  &END PRINT
&END MOTION

&FORCE_EVAL
  &DFT
    &QS
      METHOD xTB
      &XTB
        DO_EWALD F
        &PARAMETER
          DISPERSION_PARAMETER_FILE dftd3.dat
        &END PARAMETER
      &END XTB
    &END QS
    &SCF
      MAX_SCF 10
      SCF_GUESS MOPAC
      &OT
        MINIMIZER DIIS
        PRECONDITIONER FULL_SINGLE_INVERSE
      &END OT
      &OUTER_SCF
        MAX_SCF 10
      &END OUTER_SCF
    &END SCF
  &END DFT
  &SUBSYS
    &TOPOLOGY
      &CENTER_COORDINATES
      &END
    &END TOPOLOGY
    &CELL
      ABC 10.0 10.0 10.0
      PERIODIC NONE
    &END CELL
    &COORD
  O1        -0.0498339729       -0.1411658409        0.0465479157
  H1        -0.2042994664        0.7970798256       -0.1915254113
  H1         0.9067850060       -0.2661729292       -0.1371072947
  O2        -0.1404462766        0.1320823335        2.6380730506
  H2        -0.1727932227       -0.0537890285        1.6346646445
  H2         0.7341658396       -0.2033312582        2.8970132738
    &END COORD
&KIND O1
  ELEMENT O
&END KIND
&KIND H1
  ELEMENT H
&END KIND
&KIND O2
  ELEMENT O
&END KIND
&KIND H2
  ELEMENT H
&END KIND
  &END SUBSYS
&END FORCE_EVAL

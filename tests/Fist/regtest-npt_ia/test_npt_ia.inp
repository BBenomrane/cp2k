&GLOBAL
  PROJECT test_barostat 
  RUN_TYPE md
  &PRINT DEBUG 
  &END
&END GLOBAL

&MOTION
  &PRINT
   &STRUCTURE_DATA Low
     POSITION 1
   &END 
   &TRAJECTORY
     &EACH
       MD 1
     &END EACH
   &END TRAJECTORY
  &END PRINT
  &CONSTRAINT
    &FIXED_ATOMS
      Molname u
    &END FIXED_ATOMS
    &G3X3
      DISTANCES 1.808845716 1.808845716 2.860856174
      MOLNAME WAT !To be specified in the coord.inc file
      ATOMS 1 2 3
    &END G3X3
  &END CONSTRAINT
  &MD
    ENSEMBLE NPT_IA
    STEPS 3 
    TIMESTEP 1
    &THERMOSTAT
       TYPE  CSVR
       REGION  GLOBAL
       &CSVR
         TIMECON     9.9999999999999986E+01
       &END CSVR
    &END THERMOSTAT
    &BAROSTAT
      PRESSURE 1
    &END BAROSTAT
    TEMPERATURE 298
  &END MD
&END MOTION

&MULTIPLE_FORCE_EVALS
  FORCE_EVAL_ORDER  1 2 3
&END MULTIPLE_FORCE_EVALS

&FORCE_EVAL
  METHOD MIXED
  STRESS_TENSOR Analytical
  &MIXED
    GROUP_PARTITION 1 1
  # MIXING_TYPE LINEAR_COMBINATION
   MIXING_TYPE GENMIX
   &GENERIC
     # X: Energy force_eval 2
     # Y: Energy force_eval 3
     MIXING_FUNCTION X+Y
     VARIABLES X Y
   &END
  &END
  &SUBSYS
    &CELL
      ABC 32.419  32.862  31.318  !Change
      ALPHA_BETA_GAMMA 90 90 90 !Change
      PERIODIC  XYZ
    &END CELL
    &COORD
      @INCLUDE "coord.inc"
    &END COORD
    &TOPOLOGY
     &GENERATE
       BONDPARM_FACTOR 0.1 
     &END GENERATE
    &END TOPOLOGY
  &END SUBSYS
&END FORCE_EVAL

&FORCE_EVAL
   METHOD FIST
   STRESS_TENSOR Analytical
   &MM
    &FORCEFIELD
     MULTIPLE_POTENTIAL .TRUE.
     &SPLINE
       EMAX_SPLINE 10
     &END SPLINE
     IGNORE_MISSING_CRITICAL_PARAMS T
     &CHARGE
       ATOM H
       CHARGE 0.5190
     &END CHARGE
     &CHARGE
       ATOM O 
       CHARGE -1.038
     &END CHARGE
     &NONBONDED
        &LENNARD-JONES
          atoms O O
          EPSILON [kcalmol] 0.1825
          SIGMA [nm] 3.234E-01
          RCUT 10
        &END LENNARD-JONES
     &END NONBONDED
    &END FORCEFIELD
    &POISSON
      &EWALD
        EWALD_TYPE spme
        GMAX 36 36 36 !About box size
      &END EWALD
    &END POISSON
  &END MM
&END FORCE_EVAL


&FORCE_EVAL
   METHOD FIST
   STRESS_TENSOR Analytical
   &MM
    &FORCEFIELD
     &SPLINE
       EMAX_SPLINE 10
     &END SPLINE
     IGNORE_MISSING_CRITICAL_PARAMS T
     &NONBONDED
     &GAL21
         ATOMS  O Au
         METALS Au Cu
         RCUT 10
         epsilon -0.2900573456297941 3.4987906750717377 -0.9674333546865921
         bxy .4742260 .0069635
         bz .0682496 .0026624
         r 2.0278162 4.0
         a1 -99.4894825302937 27.984350833358633 -1.5525850715964307
         a2 -14.495966011655547 4.364414329846203 -0.26698882036495686
         a3 -29.59922981388242 7.860715870984457 -0.487856686201119
         a4 -4.095492196445738 -4.426681243785424e-12 0.06479847339166686
         A 4589.098759381512 1184.2605776724026
         B 3.2872089 -.0104630
         C 918.463090698739
         AH 13743.265152051408 -1286.5903894084856
         BH 4.4928678 -.1772911
         Fit_express False 
         &GCN
          @INCLUDE "gcn.inc"
         &END GCN
      &END GAL21
     &END NONBONDED
    &END FORCEFIELD
    &POISSON
      &EWALD
        EWALD_TYPE spme
        GMAX 36 36 36 !About box size
      &END EWALD
    &END POISSON
  &END MM
&END FORCE_EVAL
